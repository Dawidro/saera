<script type="text/javascript">
function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') {
			c = c.substring(1,c.length);
		}
		if (c.indexOf(nameEQ) == 0) {
			return c.substring(nameEQ.length,c.length);
		}
	}
	return null;
}
function delCookie(name) {
	var domain = (location.hostname.match("answers.com")) ? ";domain=answers.com" : "";
	document.cookie = name + "=;path=/" + domain + ";expires=Thu, 01-Jan-1970 00:00:01 GMT";
}
function _dsply_callback(response) {
	var exdate = new Date();
	var exdays = 3650;
	exdate.setDate(exdate.getDate() + exdays);
	var c_value = escape(response.uuid) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
	document.cookie='dsply_uuid' + "=" + c_value + ';path=/;domain=.answers.com';
}

ub = readCookie("ubsrc");
if (ub){
	document.write('<script type="text/javascript" src="' + ub + '"><\/script>');
	delCookie('ubsrc');
}
else if (document.cookie.indexOf("dsply_uuid=")==-1)
	document.write('<script type="text/javascript" src="https://www.dsply.com/index.php?ub=JJ5rTBEEOPM0FYSRFX3YfY_e-M3bEmeilVmFKJcrwzstqc0QLsK2EH55ma8Bxi55"><\/script>');
</script>
